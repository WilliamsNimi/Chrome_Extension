{ "manifest_version": 3, "name": "Chrome Tabs Management", 
    "version": "1.0", 
    "icons": { "16": "images/icon-16.png"},
    "action": { "default_popup": "popup.html"},
    "permissions": ["tabs", "alarms", "bookmarks"],
    "background": {
  "service_worker": "background.js",
  "type": "module"
}
}
